<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:top="@dimen/layerlist_blob_red_padding_top"
        android:left="@dimen/layerlist_blob_red_padding_left">
        <bitmap
            android:src="@drawable/blob_red_png"
            android:gravity="center"
            android:dither="false" />
    </item>

    <item
        android:top="@dimen/layerlist_blob_green_padding_top"
        android:left="@dimen/layerlist_blob_green_padding_left">
        <bitmap
            android:src="@drawable/blob_green_png"
            android:gravity="center"
            android:dither="false"/>
    </item>

    <item
        android:top="@dimen/layerlist_blob_blue_padding_top"
        android:left="@dimen/layerlist_blob_blue_padding_left">
        <bitmap
            android:src="@drawable/blob_blue_png"
            android:gravity="center"
            android:dither="false"/>
    </item>

</layer-list>