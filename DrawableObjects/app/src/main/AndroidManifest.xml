<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.luongvc.studying.drawableobjects" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".BitmapActivity"
            android:label="@string/app_bitmap" >
            <intent-filter>
                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>

        <activity
            android:name=".NinepatchActivity"
            android:label="@string/app_ninepatch" >
            <intent-filter>
                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ShapeActivity"
            android:label="@string/app_shape" >
            <intent-filter>
                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>
    </application>

</manifest>
